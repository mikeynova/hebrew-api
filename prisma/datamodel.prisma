type Link {
  id: ID! @unique
  description: String!
  url: String!
  postedBy: User
}

type User {
  id: ID! @unique
  name: String!
  email: String @unique
  password: String
  links: [Link!]!
  facebookId: String @unique
}

type Group {
  id: ID! @unique
  name: String! @unique
  lessons: [Lesson!]!
}

type Lesson {
  id: ID! @unique
  name: String!
  group: Group!
  pages: [Page!]!
}

type Page {
  id: ID! @unique
  pageNumber: Int!
  data: String!
  lesson: Lesson!
}
